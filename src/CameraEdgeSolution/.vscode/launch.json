{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CameraModule Remote Debug (.NET Core)",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickRemoteProcess}",
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "CameraModule",
          "sh",
          "-c"
        ],
        "debuggerPath": "~/vsdbg/vsdbg",
        "pipeCwd": "${workspaceFolder}",
        "quoteArgs": true
      },
      "sourceFileMap": {
        "/app": "${workspaceFolder}/modules/CameraModule"
      },
      "justMyCode": true
    },
    {
      "name": "CameraModule Local Debug (.NET Core)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "buildCameraModule",
      "program": "${workspaceRoot}/modules/CameraModule/bin/Debug/netcoreapp2.2/CameraModule.dll",
      "args": [],
      "cwd": "${workspaceRoot}/modules/CameraModule",
      "internalConsoleOptions": "openOnSessionStart",
      "stopAtEntry": false,
      "console": "internalConsole",
      "env": {
        "EdgeHubConnectionString": "${config:azure-iot-edge.EdgeHubConnectionString}",
        "webserver": "1",
        "cameraouputdirectory": "${workspaceRoot}/modules/CameraModule/testCameraOutput"
        
      }
    }
  ]
}